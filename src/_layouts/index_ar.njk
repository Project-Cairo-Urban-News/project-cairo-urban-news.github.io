   <script>
  document.addEventListener('DOMContentLoaded', function() {
    // After the page is fully loaded, re-assign the `data-letter` to each tei-item
    updateTeiItems();

    // If you need to recheck periodically, you can uncomment the following line
    // setInterval(updateTeiItems, 5000); // Optional: Reapply every 5 seconds
  });

  // Function to update the data-letter for all tei-items
  function updateTeiItems() {
    const teiItems = document.querySelectorAll('.tei-item');
    
    teiItems.forEach(item => {
      const teiName = item.querySelector('tei-name');
      if (teiName) {
        const firstLetter = teiName.textContent.trim().charAt(0); // Get the first letter of the name
        item.setAttribute('data-letter', firstLetter); // Set the data-letter attribute
      }
    });
  }

  // Function to filter items based on the selected letter
  function filterItems(letter) {
    const teiItems = document.querySelectorAll('.tei-item');
    
    teiItems.forEach(item => {
      const itemLetter = item.getAttribute('data-letter'); // Get the stored data-letter attribute
      
      if (itemLetter === letter) {
        item.style.display = ''; // Show the item
      } else {
        item.style.display = 'none'; // Hide the item
      }
    });
  }
</script>
